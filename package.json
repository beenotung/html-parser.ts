{
  "name": "html-parser.ts",
  "version": "1.9.1",
  "description": "zero-dependency html parser for node.js and browser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "npm run prettier && npm run tslint",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "tslint": "tslint -p . --fix",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "npm run format && ts-node test/index.ts"
  },
  "keywords": [
    "html",
    "parser",
    "dom",
    "server-side",
    "isomorphic",
    "typescript"
  ],
  "author": "Beeno Tung",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@types/node": "^20.10.4",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/html-parser.ts.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/html-parser.ts/issues"
  },
  "homepage": "https://github.com/beenotung/html-parser.ts#readme"
}
